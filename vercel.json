{
    "version": 2,
    "builds": [
        {
            "src": "./index.py",
            "use": "@vercel/python",
            "config": { "runtime": "python3.12" }
        }
    ],
   "routes": [
    {
        "src": "/get_blogs",
        "dest": "/get_blogs"
    },
    {
        "src": "/get_code/",
        "dest": "/get_code/",
        "headers": {
            "Location": "https://your-link-or-url.com"
        },
        "status": 301
    },
    {
        "src": "/(.*)",
        "dest": "/"
    }
]

}
